#Ecord blueprint file
#input setting
popid: cord_MR # id of the population (no white space)
nseq: 20 # number of sequences
L: 15752854 # total number of observed nucleic sites, including polymorphic and monomorphic
whether_folded: true # whethr the SFS is folded (true or false)
SFS: 101696.2171998626 122582.5098039221 144140.6759545933 142122.6738906096 136740.7892672861 131559.7115927073 150248.7395940837 94617.52012383903 0 0  # snp frequency spectrum: number of singleton, number of doubleton, etc. (separated by white space)
#smallest_size_of_SFS_bin_used_for_estimation: 1 # default is 1; to ignore singletons, uncomment this line and change this number to 2
largest_size_of_SFS_bin_used_for_estimation: 8 # default is nseq/2 for folded SFS
pct_training: 0.67 # percentage of sites for training
nrand: 5 9 14 18 # number of random break points for each try (separated by white space)
project_dir: cord_fold # project directory
stairway_plot_dir: /blue/soltis/kasey.pham/bin/stairway_plot_v2.1.1/stairway_plot_es # directory to the stairway plot files
ninput: 200 # number of input files to be created for each estimation
#random_seed: 6
#output setting
mu: 4.93e-8 # assumed mutation rate per site per generation from Silva-Junior and Grattapaglia 2015
year_per_generation: 100 # assumed generation time (in years) from Silva-Junior and Grattapaglia 2015
#plot setting
plot_title: MR E. cordata # title of the plot
xrange: 0,0 # Time (1k year) range; format: xmin,xmax; "0,0" for default
yrange: 0,0 # Ne (1k individual) range; format: xmin,xmax; "0,0" for default
xspacing: 3 # X axis spacing
yspacing: 2 # Y axis spacing
fontsize: 12 # Font size
