#example blueprint file
#input setting
popid: glob_MR # id of the population (no white space)
nseq: 40 # number of sequences
L: 8651451 # total number of observed nucleic sites, including polymorphic and monomorphic
whether_folded: true # whethr the SFS is folded (true or false)
SFS: 92561.45335770649 91909.42746993279 84987.2504982535 74276.58263620081 65373.85445545992 57580.20795794109 51198.5539268026 45495.05860438012 40231.59233138448 36637.58842640588 33956.10811245467 31860.07685388723 30707.37167475563 31301.02506590723 34347.51073462801 18295.83447183435 0 0 0 0 # snp frequency spectrum: number of singleton, number of doubleton, etc. (separated by white space)
#smallest_size_of_SFS_bin_used_for_estimation: 1 # default is 1; to ignore singletons, uncomment this line and change this number to 2
largest_size_of_SFS_bin_used_for_estimation: 16 # default is nseq/2 for folded SFS
pct_training: 0.67 # percentage of sites for training
nrand: 5 19 29 38 # number of random break points for each try (separated by white space)
