#!/bin/bash
#SBATCH --account=soltis
#SBATCH --qos=soltis-b
#SBATCH --job-name=mergevcf
#SBATCH --mail-type=ALL
#SBATCH --mail-user=kasey.pham@ufl.edu
#SBATCH --mem=15gb
#SBATCH --time=4:00:00
#SBATCH --cpus-per-task=1
#SBATCH --nodes=1
#SBATCH --output=merge_vcfs_%j.out
