pwd; hostname; date
echo Working directory is $SLURM_SUBMIT_DIR
cd $SLURM_SUBMIT_DIR

module purge
module load picard/2.25.5

picard MergeVcfs -I ../vcfs_fil.list -O all_to_ASM1654582_fil_maf0.00.vcf

